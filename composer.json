{
    "name": "luomor/larke-operation-log",
    "description": "记录 admin 系统的相关操作日志。",
    "keywords": [
        "operation-log",
        "larke-admin",
        "larke-operation-log"
    ],
    "homepage": "https://github.com/luomor-web/larke-operation-log",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Peter Zhang",
            "email": "zhangchunsheng423@gmail.com",
            "homepage": "https://github.com/zhangchunsheng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "luomor/larke-admin":"^1.6"
    },
    "autoload": {
        "psr-4": {
            "Larke\\Admin\\OperationLog\\": "src"
        }
    },
    "extra" : {
        "larke": {
            "title": "操作日志",
            "keywords": [
                "operation-log",
                "larke-admin",
                "larke-operation-log"
            ],
            "homepage": "https://github.com/deatil/larke-operation-log",
            "authors": [
                {
                    "name": "deatil",
                    "email": "deatil@github.com",
                    "homepage": "https://github.com/deatil",
                    "role": "Developer"
                }
            ],
            "version": "1.2.1",
            "adaptation": ">=1.7.2",
            "require": {}
        },
        "laravel" : {
            "providers" : [
                "Larke\\Admin\\OperationLog\\ServiceProvider"
            ]
        }
    }
}
